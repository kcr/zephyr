create_depend($(SRCS))

install_man(zephyr.1,zephyr.1)

make_err(zephyr_err)

#ifdef KERBEROS
make_err(krb_err)
install_file($(BINC)/zephyr/krb_err.h,$(ZINCDIR)/krb_err.h)
#endif

create_dir($(ZINCDIR))
install_file($(BINC)/zephyr/zephyr.h,$(ZINCDIR)/zephyr.h)
install_file($(BINC)/zephyr/zephyr_conf.h,$(ZINCDIR)/zephyr_conf.h)
